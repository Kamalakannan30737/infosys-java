import java.util.Scanner;
class typescasting{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int A=sc.nextInt();
        int reverse=0;
        while(A != 0)   
        {  
            int remainder = A % 10;  
            reverse = reverse * 10 + remainder;  
            A = A/10;  
        }
        System.out.println(reverse);
    }
}
